<% classType=instanceDataSet.instance.classtype.name %>
<% dataSetVersion=instanceDataSet.version %>
<% partialName="/instancedatasets/#{classType}/detailed_#{dataSetVersion}" %>
<% partialFileName=::Rails.root.to_s+"/app/views/instancedatasets/"+classType+"/_detailed_"+dataSetVersion.to_s+".html.erb" %>
<% if !File.exists?(partialFileName) %>
  <% partialName="instancedataset_detailed_unknown" %>
<% end %>
<%= render :partial => partialName, :locals => { :instanceDataSet => instanceDataSet } %>

