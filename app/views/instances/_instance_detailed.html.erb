<% classType=instancedata.first.instance.classtype.name %>
<% partialName="instance_detailed_#{classType}" %>
<% partialFileName=::Rails.root.to_s+"/app/views/instances/_"+partialName+".html.erb" %>
<% if !File.exists?(partialFileName) %>
  <% partialName="instance_detailed_unknown" %>
<% end %>
<%= render :partial => "instances/#{partialName}", :locals => { :instancedata => instancedata } %>

