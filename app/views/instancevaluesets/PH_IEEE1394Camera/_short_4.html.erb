<% camState=instanceValueSet.getStringParameter("Camera State") %>
<% localoptions={:width=>100, :height=>100}
localoptions.merge!(options) %>
<% if camState=="on/true" %>
  <%= displayParameter(instanceValueSet,"Image",localoptions) %>
<% elsif camState=="off/false" %>
  <%= image_tag "icon_OFF.png" %>
<% elsif camState=="Failure" %>
  <%= image_tag "icon_ERROR.png" %>
<% else %>
  <%= image_tag "icon_UNKNOWN.png" %>
<% end %>
