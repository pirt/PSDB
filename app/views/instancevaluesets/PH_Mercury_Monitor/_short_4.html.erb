<% onState=getStringParameter(instanceValueSet.instancevalues,"Status On/Off",:upcase=>true)
if onState="OFF/FALSE"
  iconName="icon_OFF.png"
else
  alarmState=getStringParameter(instanceValueSet.instancevalues,"State Alarm",:upcase=>true)
  if alarmState="OFF/FALSE"
    iconName="icon_OK.png"
  else
    iconName="icon_ERROR.png"
  end
end %>
<%= image_tag iconName, {:style=>"vertical-align:middle"} %>
