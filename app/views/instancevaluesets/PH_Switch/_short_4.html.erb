<%=
  status=instanceValueSet.getStringParameter("Status",:upcase=>true)
  closedRegEx=/^(.* )?(IN|CLOSED)(.)*/
  openRegEx=/^(.* )?(OUT|OPEN)(.)*/
  if !closedRegEx.match(status).nil?
    iconName="icon_IN.png"
  elsif !openRegEx.match(status).nil?
    iconName="icon_OUT.png"
  else
    case status
      when "FAILURE"
        iconName="icon_ERROR.png"
      else
        iconName="icon_UNKNOWN.png"
    end
  end
  image_tag iconName, {:style=>"vertical-align:middle"} %>
<strong><%= displayParameter(instanceValueSet,"Status") %></strong>
