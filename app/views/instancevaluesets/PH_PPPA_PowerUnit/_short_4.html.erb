<% puOn=instanceValueSet.getBooleanParameter("PU in use?")
if puOn
  puStatus=instanceValueSet.getStringParameter("Machine State",:upcase=>true)
  if puStatus="STAND BY"
    voltage=instanceValueSet.getNumericParameter("Voltage")/1000.0 %>
    <%= number_with_precision(voltage, :precision => 1) %> kV
<% else %>
<%= image_tag "icon_ERROR.png", {:style=>"vertical-align:middle"} %>
<% end %>
<% else %>
<%= image_tag "icon_OFF.png", {:style=>"vertical-align:middle"} %>
<% end %>

