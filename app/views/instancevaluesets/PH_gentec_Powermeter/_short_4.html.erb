<% statusData=instanceValueSet.instancevalues.find_by_name("Device State")
  if !statusData.nil?
    deviceStatus=statusData.data_string.upcase
   else
    deviceStatus=""
   end %>
<% if deviceStatus=="ON/TRUE" %>
  <%= displayParameter(instanceValueSet.instancevalues,"Value") %>
<% else %>
  <%= image_tag("icon_ERROR.png") %>
<% end %>
