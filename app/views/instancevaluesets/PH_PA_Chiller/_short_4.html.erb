<%=
  conductivity=instanceValueSet.getStringParameter("Conductivity",:upcase=>true)=="ON/TRUE" ? true : false
  niveau=instanceValueSet.getStringParameter("Niveau",:upcase=>true)=="ON/TRUE" ? true : false
  flow=instanceValueSet.getStringParameter("Flow",:upcase=>true)=="ON/TRUE" ? true : false
  temperature=instanceValueSet.getStringParameter("High Temperature",:upcase=>true)=="ON/TRUE" ? true : false
  if conductivity and niveau and flow and temperature
    iconName="icon_OK.png"
  elsif !conductivity and niveau and !flow and !temperature
    iconName="icon_OFF.png"
  else
    iconName="icon_ERROR.png"
  end
  image_tag iconName %>
