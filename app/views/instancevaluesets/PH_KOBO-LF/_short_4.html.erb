<%=
  device=instanceValueSet.getStringParameter("Device Status",:upcase=>true)=="ON/TRUE" ? true : false
  on=instanceValueSet.getStringParameter("On?",:upcase=>true)=="ON/TRUE" ? true : false
  conductivityOK=instanceValueSet.getStringParameter("Conductivity to high?",:upcase=>true)=="ON/TRUE" ? false : true
  alarm=instanceValueSet.getStringParameter("Alarm?",:upcase=>true)=="ON/TRUE" ? true : false
  temperatureOK=instanceValueSet.getStringParameter("Temperature not to high?",:upcase=>true)=="ON/TRUE" ? false : true
  if device and on and conductivityOK and temperatureOK and !alarm
    iconName="icon_OK.png"
  elsif device and !on
    iconName="icon_OFF.png"
  else
    iconName="icon_ERROR.png"
  end
  image_tag iconName, {:style=>"vertical-align:middle"}
%>
