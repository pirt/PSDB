<% if instancedata.nil? %>
  <%= "parameter does not exist!" %>
<% else %>
  <% dataType=instancedata.datatype.name %>
  <% partialName="instancedata_#{dataType}" %>
  <% partialFileName=::Rails.root.to_s+"/app/views/instancedatas/_"+partialName+".html.erb" %>
  <% if !File.exists?(partialFileName) %>
    <% partialName="instancedata_unknown" %>
  <% end %>
  <% localoptions = {} %>
  <% if defined?(options) %>
    <% localoptions=localoptions.merge(options) %>
  <% end %>
  <%= render :partial => "instancedatas/#{partialName}", 
      :locals => { :instancedata => instancedata, :options => localoptions } %>
<% end %>
